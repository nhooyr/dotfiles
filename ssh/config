Host *
ControlPath ~/.ssh/sockets/%r@%n
ControlMaster auto
ControlPersist yes
ServerAliveInterval 5
TCPKeepAlive no
ExitOnForwardFailure yes
# Removes "Shared connection to" messages
LogLevel=QUIET
StrictHostKeyChecking accept-new
ConnectTimeout 5
AddKeysToAgent yes

Include /Users/nhooyr/src/nhooyr/dotfiles/secrets/ssh/config
