source_if_exists() {
	if [[ -f $1 ]]; then
	  source "$@"	
	fi
}

source ~/src/nhooyr/dotfiles/zsh/alias.zsh
source ~/src/nhooyr/dotfiles/zsh/completion.zsh
source ~/src/nhooyr/dotfiles/zsh/env.zsh
source ~/src/nhooyr/dotfiles/zsh/history.zsh
source ~/src/nhooyr/dotfiles/zsh/misc.zsh
source ~/src/nhooyr/dotfiles/zsh/prompt.zsh
source ~/src/nhooyr/dotfiles/zsh/syntax.zsh
